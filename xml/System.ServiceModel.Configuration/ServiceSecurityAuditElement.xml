      <MemberSignature Language="ILAsm" Value=".property instance bool SuppressAuditFailure" />
      <MemberSignature Language="DocId" Value="P:System.ServiceModel.Configuration.ServiceSecurityAuditElement.SuppressAuditFailure" />
